{
  "manifest_version": 2,

  "name": "UKConverter",
  "description": "This extension replaces pounds(€) or Euros(£) to the correct US dollar amount. ",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}